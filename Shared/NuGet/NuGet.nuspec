<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.Data</id>
    <version>4.2.187</version>
    <title>SQLite data for Zebble (Xamarin)</title>
    <description>Provides entities and sql data support for Zebble.</description>
    <iconUrl>https://raw.githubusercontent.com/Geeksltd/Zebble.Data/master/Shared/Icon.png</iconUrl>
    <projectUrl>https://github.com/Geeksltd/Zebble.Data</projectUrl>
    <authors>Mobile App Technologies Ltd</authors>
    <owners>Mobile App Technologies Ltd</owners>
    <copyright>Copyright ©2017 Mobile App Technologies Ltd - All rights reserved.</copyright>
    <dependencies>
      <group targetFramework="UAP10">
        <dependency id="System.Data.Common" version="4.3.0" />
        <dependency id="System.Data.SqlClient" version="4.3.0" />
      </group>
      <group targetFramework="MonoAndroid">
        <dependency id="System.Data.Common" version="4.3.0" />
        <dependency id="Mono.Data.Sqlite.Portable" version="1.0.3.5" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="System.Data.Common" version="4.3.0" />
        <dependency id="Mono.Data.Sqlite.Portable" version="1.0.3.5" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\lib\Zebble.Data.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\..\lib\Zebble.Data.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\Zebble.Data.UWP.*" target="lib\UAP10\" />
  </files>
</package>